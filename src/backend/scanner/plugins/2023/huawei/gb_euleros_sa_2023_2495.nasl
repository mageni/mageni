  if(!isnull(res = isrpmvuln(pkg:"systemd-nspawn", rpm:"systemd-nspawn~243~31.h78.eulerosv2r10", rls:"EULEROSVIRT-2.10.0"))) {
    report += res;
  }

  if(!isnull(res = isrpmvuln(pkg:"systemd-pam", rpm:"systemd-pam~243~31.h78.eulerosv2r10", rls:"EULEROSVIRT-2.10.0"))) {
    report += res;

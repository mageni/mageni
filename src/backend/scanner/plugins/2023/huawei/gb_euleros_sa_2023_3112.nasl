  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2023-08-04 15:21:45 +0000 (Fri, 04 Aug 2023)");

  script_name("Huawei EulerOS: Security Advisory for amanda (EulerOS-SA-2023-3112)");

  script_tag(name:"severity_origin", value:"NVD");
  script_tag(name:"severity_date", value:"2023-02-01 19:16:00 +0000 (Wed, 01 Feb 2023)");

  script_name("Huawei EulerOS: Security Advisory for expat (EulerOS-SA-2023-2219)");

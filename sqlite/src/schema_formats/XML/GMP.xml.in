          </column>
          <column>
            <name>trend</name>
            <type>
              <alts>
                <alt>up</alt>
                <alt>down</alt>
                <alt>more</alt>
                <alt>less</alt>
                <alt>same</alt>
              </alts>
            </type>
            <summary>Trend of the task</summary>

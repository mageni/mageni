            <type>oid</type>
            <summary>Alias of nvt</summary>
          </column>
          <column>
            <name>task_name</name>
            <type>text</type>
            <summary>Name of the Task the Note applies to</summary>
          </column>
          <column>
            <name>task_id</name>
            <type>uuid</type>
            <summary>UUID of the Task the Note applies to</summary>
          </column>
          <column>
            <name>hosts</name>
            <type>text</type>
            <summary>Hosts the Note applies to</summary>
          </column>
          <column>
            <name>port</name>
            <type>text</type>
            <summary>Port the Note applies to</summary>
          </column>

            <summary>Total number of hosts</summary>
          </column>
          <column>
            <name>result_hosts</name>
            <type>integer</type>
            <summary>Number of hosts with results matching the min_qod</summary>
          </column>
          <column>
            <name>fp_per_host</name>
            <type>integer</type>
            <summary>Number of false positive results per host with results</summary>
          </column>
          <column>
            <name>log_per_host</name>
            <type>integer</type>
            <summary>Number of log results per host with results</summary>
          </column>
          <column>
            <name>low_per_host</name>
            <type>integer</type>
            <summary>Number of low severity results per host with results</summary>
          </column>
          <column>
            <name>medium_per_host</name>
            <type>integer</type>
            <summary>Number of medium severity results per host with results</summary>
          </column>
          <column>
            <name>high_per_host</name>
            <type>integer</type>
            <summary>Number of high severity results per host with results</summary>
          </column>
          <column>
            <name>target</name>
            <type>name</type>
            <summary>Name of the target</summary>
          </column>
        </filter_keywords>
      </attrib>
      <attrib>
        <name>filt_id</name>
        <summary>ID of filter to use to filter query</summary>
        <type>uuid</type>
      </attrib>
      <attrib>
        <name>trash</name>
        <summary>Whether to get the trashcan tasks instead</summary>
        <type>boolean</type>
      </attrib>
      <attrib>
        <name>details</name>
        <summary>Whether to include full task details</summary>
        <type>boolean</type>
      </attrib>
      <attrib>
        <name>ignore_pagination</name>
        <summary>
          Whether to ignore info used to split the report into pages
          like the filter terms "first" and "rows".
        </summary>
        <type>boolean</type>
      </attrib>
      <attrib>
        <name>schedules_only</name>
        <summary>Whether to only include id, name and schedule details</summary>
        <type>boolean</type>
      </attrib>
    </pattern>
    <response>
      <pattern>
        <attrib>
          <name>status</name>
          <type>status</type>
          <required>1</required>
        </attrib>
        <attrib>
          <name>status_text</name>
          <type>text</type>
          <required>1</required>
        </attrib>
        <e>apply_overrides</e>
        <any><e>task</e></any>
        <e>filters</e>
        <e>sort</e>
        <e>tasks</e>
        <e>task_count</e>
      </pattern>
      <ele>
        <name>apply_overrides</name>
        <pattern>
          <t><alts><alt>0</alt><alt>1</alt></alts></t>
        </pattern>
      </ele>
      <ele>
        <name>task</name>
        <pattern>
          <attrib>
            <name>id</name>
            <type>uuid</type>
            <required>1</required>
          </attrib>
          <or>
            <g>
              <e>owner</e>
              <e>name</e>
              <e>comment</e>
              <e>creation_time</e>
              <e>modification_time</e>
              <e>writable</e>
              <e>in_use</e>
              <e>permissions</e>
              <o><e>user_tags</e></o>
              <e>status</e>
              <e>progress</e>
              <e>alterable</e>
              <e>config</e>
              <e>target</e>
              <e>hosts_ordering</e>
              <e>scanner</e>
              <e>alert</e>
              <e>observers</e>
              <e>schedule</e>
              <e>schedule_periods</e>
              <e>report_count</e>
              <e>trend</e>
              <o><e>current_report</e></o>
              <o><e>first_report</e></o>
              <o><e>last_report</e></o>
              <o><e>second_last_report</e></o>
              <any><e>reports</e></any>
              <o><e>average_duration</e></o>
              <e>result_count</e>
              <e>preferences</e>
            </g>
            <g>
              <e>name</e>
              <e>schedule</e>
              <e>schedule_periods</e>
            </g>
          </or>
        </pattern>
        <ele>
          <name>owner</name>
          <summary>Owner of the task</summary>
          <pattern>
            <e>name</e>
          </pattern>
          <ele>
            <name>name</name>
            <summary>The name of the owner</summary>
            <pattern><t>name</t></pattern>
          </ele>
        </ele>
        <ele>
          <name>name</name>
          <summary>The name of the task</summary>
          <pattern><t>name</t></pattern>
        </ele>
        <ele>
          <name>comment</name>
          <summary>The comment on the task</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>creation_time</name>
          <summary>Creation time of the task</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>modification_time</name>
          <summary>Last time the task was modified</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>writable</name>
          <summary>Whether the task is writable</summary>
          <pattern><t>boolean</t></pattern>
        </ele>
        <ele>
          <name>in_use</name>
          <summary>Whether this task is currently in use</summary>
          <pattern><t>boolean</t></pattern>
        </ele>
        <ele>
          <name>permissions</name>
          <summary>
            Permissions that the current user has on the task
          </summary>
          <pattern>
            <any><e>permission</e></any>
          </pattern>
          <ele>
            <name>permission</name>
            <pattern>
              <e>name</e>
            </pattern>
            <ele>
              <name>name</name>
              <summary>The name of the permission</summary>
              <pattern><t>name</t></pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>user_tags</name>
          <summary>Info on tags attached to the task</summary>
          <pattern>
            <e>count</e>
            <any><e>tag</e></any>
          </pattern>
          <ele>
            <name>count</name>
            <summary>Number of attached tags</summary>
            <pattern><t>integer</t></pattern>
          </ele>
          <ele>
            <name>tag</name>
            <summary>
              Short info on an individual tag
              (only if details were requested)
            </summary>
            <pattern>
              <attrib>
                <name>id</name>
                <type>uuid</type>
                <summary>UUID of the tag</summary>
                <required>1</required>
              </attrib>
              <e>name</e>
              <e>value</e>
              <e>comment</e>
            </pattern>
            <ele>
              <name>name</name>
              <summary>Name of the tag (usually namespace:predicate)</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>value</name>
              <summary>Value of the tag</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>comment</name>
              <summary>Comment for the tag</summary>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>status</name>
          <summary>The run status of the task</summary>
          <pattern><t>task_status</t></pattern>
        </ele>
        <ele>
          <name>progress</name>
          <summary>The percentage of the task that is complete</summary>
          <pattern>
            <t>integer</t>
            <any><e>host_progress</e></any>
          </pattern>
          <ele>
            <name>host_progress</name>
            <summary>
              Percentage complete for a particular host,
              only shown when details are active</summary>
            <pattern>
              <t>integer</t>
              <e>host</e>
            </pattern>
            <ele>
              <name>host</name>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>alterable</name>
          <summary>Whether the task is an Alterable Task</summary>
          <pattern><t>boolean</t></pattern>
        </ele>
        <ele>
          <name>config</name>
          <summary>The scan configuration used by the task</summary>
          <pattern>
            <attrib>
              <name>id</name>
              <type>uuid</type>
              <required>1</required>
            </attrib>
            <e>name</e>
            <o><e>permissions</e></o>
            <e>trash</e>
          </pattern>
          <ele>
            <name>name</name>
            <summary>The name of the config</summary>
            <pattern><t>name</t></pattern>
          </ele>
          <ele>
            <name>permissions</name>
            <summary>Permissions the user has on the config</summary>
            <pattern></pattern>
          </ele>
          <ele>
            <name>trash</name>
            <summary>Whether the config is in the trashcan</summary>
            <pattern><t>boolean</t></pattern>
          </ele>
        </ele>
        <ele>
          <name>target</name>
          <summary>The hosts scanned by the task</summary>
          <pattern>
            <attrib>
              <name>id</name>
              <type>uuid</type>
              <required>1</required>
            </attrib>
            <e>name</e>
            <o><e>permissions</e></o>
            <e>trash</e>
          </pattern>
          <ele>
            <name>name</name>
            <summary>The name of the target</summary>
            <pattern><t>name</t></pattern>
          </ele>
          <ele>
            <name>permissions</name>
            <summary>Permissions the user has on the target</summary>
            <pattern></pattern>
          </ele>
          <ele>
            <name>trash</name>
            <summary>Whether the target is in the trashcan</summary>
            <pattern><t>boolean</t></pattern>
          </ele>
        </ele>
        <ele>

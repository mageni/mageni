        <type>boolean</type>
      </attrib>
      <attrib>
        <name>names_only</name>
        <summary>Whether to get only distinct tag names</summary>
        <type>boolean</type>
      </attrib>
    </pattern>
    <response>
      <pattern>
        <attrib>
          <name>status</name>
          <type>status</type>
          <required>1</required>
        </attrib>
        <attrib>
          <name>status_text</name>
          <type>text</type>
          <required>1</required>
        </attrib>
        <any><e>tag</e></any>
        <e>filters</e>
        <e>sort</e>
        <e>tags</e>
        <e>tag_count</e>
      </pattern>
      <ele>
        <name>tag</name>
        <summary>Information on a single tag</summary>
        <pattern>
          <attrib>
            <name>id</name>
            <summary>uuid of the tag (omitted when using names_only)</summary>
            <type>uuid</type>
          </attrib>
          <e>name</e>
          <o>
            <g>
              <e>permissions</e>
              <e>owner</e>
              <e>comment</e>
              <e>creation_time</e>
              <e>modification_time</e>
              <e>writable</e>
              <e>in_use</e>
              <e>resources</e>
              <e>value</e>
              <e>active</e>
              <e>orphan</e>
            </g>

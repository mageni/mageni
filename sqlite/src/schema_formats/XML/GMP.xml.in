  <command>
    <name>get_tickets</name>
    <summary>Get one or many tickets</summary>
    <description>
      The client uses the get_tickets command to get ticket information.
    </description>
    <pattern>
      <attrib>
        <name>ticket_id</name>
        <summary>ID of single ticket to get</summary>
        <type>uuid</type>
      </attrib>
      <attrib>
        <name>filter</name>
        <summary>Filter term to use to filter query</summary>
        <type>text</type>
        <filter_keywords>
          <column>
            <name>uuid</name>
            <type>uuid</type>
            <summary>Unique ID</summary>
          </column>
          <column>
            <name>name</name>
            <type>name</type>
            <summary>Name</summary>
          </column>
          <column>
            <name>comment</name>
            <type>text</type>
            <summary>Comment text</summary>
          </column>
          <column>
            <name>created</name>
            <type>iso_time</type>
            <summary>Creation time</summary>
          </column>
          <column>
            <name>modified</name>
            <type>iso_time</type>
            <summary>Modification time</summary>
          </column>
          <column>
            <name>owner</name>
            <type>name</type>
            <summary>Name of the owner</summary>
          </column>
          <column>
            <name>severity</name>
            <type>severity</type>
            <summary>Severity of the ticket result</summary>
          </column>
          <column>
            <name>host</name>
            <type>text</type>
            <summary>Host of the ticket result</summary>
          </column>
          <column>
            <name>location</name>
            <type>text</type>
            <summary>Location of the ticket result</summary>
          </column>
          <column>
            <name>solution_type</name>
            <type>text</type>
            <summary>Solution type of the ticket result</summary>
          </column>
          <column>
            <name>status</name>
            <type>ticket_status</type>
            <summary>Ticket status</summary>
          </column>
          <column>
            <name>opened</name>
            <type>iso_time</type>
            <summary>Time ticket was opened</summary>
          </column>
          <column>
            <name>fixed</name>
            <type>iso_time</type>
            <summary>Time ticket was fixed</summary>
          </column>
          <column>
            <name>closed</name>
            <type>iso_time</type>
            <summary>Time ticket was closed</summary>
          </column>
        </filter_keywords>
      </attrib>
      <attrib>
        <name>filt_id</name>
        <summary>ID of filter to use to filter query</summary>
        <type>uuid</type>
      </attrib>
      <attrib>
        <name>trash</name>
        <summary>Whether to get the trashcan tickets instead</summary>
        <type>boolean</type>
      </attrib>
    </pattern>
    <response>
      <pattern>
        <attrib>
          <name>status</name>
          <type>status</type>
          <required>1</required>
        </attrib>
        <attrib>
          <name>status_text</name>
          <type>text</type>
          <required>1</required>
        </attrib>
        <any><e>ticket</e></any>
        <e>filters</e>
        <e>sort</e>
        <e>tickets</e>
        <e>ticket_count</e>
      </pattern>
      <ele>
        <name>ticket</name>
        <pattern>
          <attrib>
            <name>id</name>
            <type>uuid</type>
            <required>1</required>
          </attrib>
          <e>owner</e>
          <e>name</e>
          <e>comment</e>
          <e>creation_time</e>
          <e>modification_time</e>
          <e>writable</e>
          <e>in_use</e>
          <e>permissions</e>
          <o><e>user_tags</e></o>
          <e>nvt</e>
          <o><e>result</e></o>
          <e>assigned_to</e>
          <o><e>task</e></o>
          <o><e>report</e></o>
          <e>severity</e>
          <e>host</e>
          <e>location</e>
          <e>solution_type</e>
          <e>status</e>
          <e>open_time</e>
          <e>open_note</e>
          <o><e>fixed_time</e></o>
          <o><e>fixed_note</e></o>
          <o><e>closed_time</e></o>
          <o><e>closed_note</e></o>
          <o><e>fix_verified_time</e></o>
          <o><e>fix_verified_report</e></o>
          <e>orphan</e>
        </pattern>
        <ele>
          <name>owner</name>
          <summary>Owner of the ticket</summary>
          <pattern>
            <e>name</e>
          </pattern>
          <ele>
            <name>name</name>
            <summary>The name of the owner</summary>
            <pattern><t>name</t></pattern>
          </ele>
        </ele>
        <ele>
          <name>name</name>
          <summary>The name of ticket result's NVT</summary>
          <pattern><t>name</t></pattern>
        </ele>
        <ele>
          <name>comment</name>
          <summary>The comment on the ticket</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>creation_time</name>
          <summary>Date and time the ticket was created</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>modification_time</name>
          <summary>Date and time the ticket was last modified</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>writable</name>
          <summary>Whether the ticket is writable</summary>
          <pattern><t>boolean</t></pattern>
        </ele>
        <ele>
          <name>in_use</name>
          <summary>Whether the ticket is in use</summary>
          <pattern><t>boolean</t></pattern>
        </ele>
        <ele>
          <name>permissions</name>
          <summary>
            Permissions that the current user has on the ticket
          </summary>
          <pattern>
            <any><e>permission</e></any>
          </pattern>
          <ele>
            <name>permission</name>
            <pattern>
              <e>name</e>
            </pattern>
            <ele>
              <name>name</name>
              <summary>The name of the permission</summary>
              <pattern><t>name</t></pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>user_tags</name>
          <summary>Info on tags attached to the ticket</summary>
          <pattern>
            <e>count</e>
            <any><e>tag</e></any>
          </pattern>
          <ele>
            <name>count</name>
            <summary>Number of attached tags</summary>
            <pattern><t>integer</t></pattern>
          </ele>
          <ele>
            <name>tag</name>
            <summary>
              Short info on an individual tag
              (only if details were requested)
            </summary>
            <pattern>
              <attrib>
                <name>id</name>
                <type>uuid</type>
                <summary>UUID of the tag</summary>
                <required>1</required>
              </attrib>
              <e>name</e>
              <e>value</e>
              <e>comment</e>
            </pattern>
            <ele>
              <name>name</name>
              <summary>Name of the tag (usually namespace:predicate)</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>value</name>
              <summary>Value of the tag</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>comment</name>
              <summary>Comment for the tag</summary>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>nvt</name>
          <summary>The NVT the ticket refers to</summary>
          <pattern>
            <attrib>
              <name>oid</name>
              <type>oid</type>
              <required>1</required>
              <summary>OID of the NVT</summary>
            </attrib>
          </pattern>
        </ele>
        <ele>
          <name>result</name>
          <summary>The ticket's result</summary>
          <pattern>
            <attrib>
              <name>id</name>
              <type>uuid</type>
              <required>1</required>
            </attrib>
          </pattern>
        </ele>
        <ele>
          <name>assigned_to</name>
          <pattern>
            <e>user</e>
          </pattern>
          <ele>
            <name>user</name>
            <summary>The user the ticket is assigned to</summary>
            <pattern>
              <attrib>
                <name>id</name>
                <type>uuid</type>
                <required>1</required>
              </attrib>
            </pattern>
          </ele>
        </ele>
        <ele>
          <name>task</name>
          <summary>The ticket's task</summary>
          <pattern>
            <attrib>
              <name>id</name>
              <type>uuid</type>
              <required>1</required>
            </attrib>
          </pattern>
        </ele>
        <ele>
          <name>report</name>
          <summary>The ticket's report</summary>
          <pattern>
            <attrib>
              <name>id</name>
              <type>uuid</type>
              <required>1</required>
            </attrib>
          </pattern>
        </ele>
        <ele>
          <name>severity</name>
          <summary>The ticket result's severity</summary>
          <pattern>
            <t>severity</t>
          </pattern>
        </ele>
        <ele>
          <name>host</name>
          <summary>The ticket result's host</summary>
          <pattern>
            text
          </pattern>
        </ele>
        <ele>
          <name>location</name>
          <summary>The ticket result's location</summary>
          <pattern>
            text
          </pattern>
        </ele>
        <ele>
          <name>solution_type</name>
          <summary>The ticket result's solution_type</summary>
          <pattern>
            text
          </pattern>
        </ele>
        <ele>
          <name>status</name>
          <summary>The ticket's status</summary>

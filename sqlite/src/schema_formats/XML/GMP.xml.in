            </ele>
            <ele>
              <name>comment</name>
              <summary>Comment for the tag</summary>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
        <ele>
          <name>first_time</name>
          <summary>First time the task ran or will run</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>next_time</name>
          <summary>The next time the task will run</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>icalendar</name>
          <summary>iCalendar text containing the time data.</summary>
          <pattern>
            text
          </pattern>
        </ele>
        <ele>
          <name>timezone</name>
          <summary>The timezone that the schedule follows</summary>
          <pattern>timezone</pattern>
        </ele>
        <ele>
          <name>timezone_abbrev</name>
          <summary>The abbreviated name of the timezone</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>period</name>
          <summary>How often the task will run, in seconds</summary>
          <pattern><t>integer</t></pattern>
        </ele>
        <ele>
          <name>period_months</name>
          <summary>How often the task will run, months portion</summary>
          <pattern><t>integer</t></pattern>
        </ele>
        <ele>
          <name>simple_period</name>
          <summary>Simple representation of period, if available</summary>
          <pattern>
            text
            <o><e>unit</e></o>
          </pattern>
          <ele>
            <name>unit</name>
            <pattern><t>time_unit</t></pattern>
          </ele>
        </ele>
        <ele>
          <name>duration</name>
          <summary>How long the task will run for (0 for entire task)</summary>
          <pattern><t>integer</t></pattern>
        </ele>
        <ele>
          <name>simple_duration</name>
          <summary>Simple representation of duration, if available</summary>
          <pattern>
            text
            <o><e>unit</e></o>
          </pattern>
          <ele>
            <name>unit</name>
            <pattern><t>time_unit</t></pattern>
          </ele>
        </ele>
        <ele>
          <name>tasks</name>
          <pattern>
            <any><e>task</e></any>
          </pattern>
          <ele>
            <name>task</name>
            <pattern>
              <attrib>
                <name>id</name>
                <type>uuid</type>
                <required>1</required>
              </attrib>
              <e>name</e>
              <o><e>permissions</e></o>
            </pattern>
            <ele>
              <name>name</name>
              <summary>The name of the task</summary>
              <pattern><t>name</t></pattern>
            </ele>
            <ele>
              <name>permissions</name>
              <summary>Permissions the user has on the task</summary>
              <pattern></pattern>
            </ele>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>filters</name>
        <pattern>
          <attrib>
            <name>id</name>
            <summary>UUID of filter if any, else 0</summary>
            <type>uuid</type>
            <required>1</required>
          </attrib>
          <e>term</e>
          <o><e>name</e></o>
          <e>keywords</e>
        </pattern>
        <ele>
          <name>term</name>
          <summary>Filter term</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>name</name>
          <summary>Filter name, if applicable</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>keywords</name>
          <summary>Filter broken down into keywords</summary>
          <pattern><any><e>keyword</e></any></pattern>
          <ele>
            <name>keyword</name>
            <pattern>
              <e>column</e>
              <e>relation</e>
              <e>value</e>
            </pattern>
            <ele>
              <name>column</name>
              <summary>Column prefix</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>relation</name>
              <summary>Relation operator</summary>
              <pattern>
                <alts>
                  <alt>=</alt>
                  <alt>:</alt>
                  <alt>~</alt>
                  <alt>&gt;</alt>
                  <alt>&lt;</alt>
                </alts>
              </pattern>
            </ele>
            <ele>
              <name>value</name>
              <summary>The filter text</summary>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>sort</name>
        <pattern>
          text
          <e>field</e>
        </pattern>
        <ele>
          <name>field</name>
          <pattern>
            <e>order</e>
          </pattern>
          <ele>
            <name>order</name>
            <pattern>
              <t>
                <alts>
                  <alt>ascending</alt>
                  <alt>descending</alt>
                </alts>
              </t>
            </pattern>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>schedules</name>
        <pattern>
          <attrib>
            <name>start</name>
            <summary>First schedule</summary>
            <type>integer</type>
            <required>1</required>
          </attrib>
          <attrib>
            <name>max</name>
            <summary>Maximum number of schedules</summary>
            <type>integer</type>
            <required>1</required>
          </attrib>
        </pattern>
      </ele>
      <ele>
        <name>schedule_count</name>
        <pattern>
          <e>filtered</e>
          <e>page</e>
        </pattern>
        <ele>
          <name>filtered</name>
          <summary>Number of schedules after filtering</summary>
          <pattern><t>integer</t></pattern>
        </ele>
        <ele>
          <name>page</name>
          <summary>Number of schedules on current page</summary>
          <pattern><t>integer</t></pattern>
        </ele>
      </ele>
    </response>
    <example>
      <summary>Get full details of a single schedule</summary>
      <request>
        <get_schedules schedule_id="c33864a9-d3fd-44b3-8717-972bfb01dfcf">
        </get_schedules>
      </request>
      <response>
        <get_schedules_response status="200" status_text="OK">
          <schedule id="c33864a9-d3fd-44b3-8717-972bfb01dfcf">

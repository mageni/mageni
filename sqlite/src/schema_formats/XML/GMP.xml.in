      <ele>
        <name>hour</name>
        <pattern><t>integer</t></pattern>
      </ele>
      <ele>
        <name>day_of_month</name>
        <pattern><t>integer</t></pattern>
      </ele>
      <ele>
        <name>month</name>
        <pattern><t>integer</t></pattern>

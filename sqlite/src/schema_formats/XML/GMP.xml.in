          <pattern>
            <alts>
              <alt>nvt</alt>
              <alt>cve</alt>
              <alt>ovaldef</alt>
            </alts>
          </pattern>
        </ele>
        <ele>
          <name>creation_time</name>
          <summary>Creation time of the user</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>modification_time</name>
          <summary>Last time the user was modified</summary>
          <pattern><t>iso_time</t></pattern>
        </ele>
        <ele>
          <name>severity</name>
          <summary>Severity of the vulnerability</summary>
          <pattern>
            <t>severity</t>
          </pattern>
        </ele>
        <ele>
          <name>qod</name>
          <summary>Quality of detection (QoD) of the vulnerability</summary>
          <pattern>
            <t>integer</t>
          </pattern>
        </ele>
        <ele>
          <name>results</name>
          <pattern>
            <e>count</e>
            <e>oldest</e>
            <e>newest</e>
          </pattern>
          <ele>
            <name>count</name>
            <summary>The number of results for the vulnerability</summary>
            <pattern>
              <t>integer</t>
            </pattern>
          </ele>
          <ele>
            <name>oldest</name>
            <summary>Timestamp of the oldest result</summary>
            <pattern>
              <t>iso_time</t>
            </pattern>
          </ele>
          <ele>
            <name>newest</name>
            <summary>Timestamp of the newest result</summary>
            <pattern>
              <t>iso_time</t>
            </pattern>
          </ele>
        </ele>
        <ele>
          <name>hosts</name>
          <pattern>
            <e>count</e>
          </pattern>
          <ele>
            <name>count</name>
            <summary>
              The number of hosts where the vulnerability was detected
            </summary>
            <pattern>
              <t>integer</t>
            </pattern>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>filters</name>
        <pattern>
          <attrib>
            <name>id</name>
            <summary>UUID of filter if any, else 0</summary>
            <type>uuid</type>
            <required>1</required>
          </attrib>
          <e>term</e>
          <o><e>name</e></o>
          <e>keywords</e>
        </pattern>
        <ele>
          <name>term</name>
          <summary>Filter term</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>name</name>
          <summary>Filter name, if applicable</summary>
          <pattern>text</pattern>
        </ele>
        <ele>
          <name>keywords</name>
          <summary>Filter broken down into keywords</summary>
          <pattern><any><e>keyword</e></any></pattern>
          <ele>
            <name>keyword</name>
            <pattern>
              <e>column</e>
              <e>relation</e>
              <e>value</e>
            </pattern>
            <ele>
              <name>column</name>
              <summary>Column prefix</summary>
              <pattern>text</pattern>
            </ele>
            <ele>
              <name>relation</name>
              <summary>Relation operator</summary>
              <pattern>
                <alts>
                  <alt>=</alt>
                  <alt>:</alt>
                  <alt>~</alt>
                  <alt>&gt;</alt>
                  <alt>&lt;</alt>
                </alts>
              </pattern>
            </ele>
            <ele>
              <name>value</name>
              <summary>The filter text</summary>
              <pattern>text</pattern>
            </ele>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>sort</name>
        <pattern>
          text
          <e>field</e>
        </pattern>
        <ele>
          <name>field</name>
          <pattern>
            <e>order</e>
          </pattern>
          <ele>
            <name>order</name>
            <pattern>
              <t>
                <alts>
                  <alt>ascending</alt>
                  <alt>descending</alt>
                </alts>
              </t>
            </pattern>
          </ele>
        </ele>
      </ele>
      <ele>
        <name>vulns</name>
        <pattern>
          <attrib>
            <name>start</name>
            <summary>First vulnerability</summary>
            <type>integer</type>
            <required>1</required>
          </attrib>
          <attrib>
            <name>max</name>
            <summary>Maximum number of vulnerabilities</summary>
            <type>integer</type>
            <required>1</required>
          </attrib>
        </pattern>
      </ele>
      <ele>
        <name>vuln_count</name>
        <pattern>
          <e>filtered</e>
          <e>page</e>
        </pattern>
        <ele>
          <name>filtered</name>
          <summary>Number of vulnerabilities after filtering</summary>
          <pattern><t>integer</t></pattern>
        </ele>
        <ele>
          <name>page</name>
          <summary>Number of vulnerabilities on current page</summary>
          <pattern><t>integer</t></pattern>
        </ele>
      </ele>
    </response>
    <example>
      <summary>Get the vulnerabilities</summary>
      <request>
        <get_vulns></get_vulns>
      </request>
      <response>
        <get_vulns_response status="200" status_text="OK">
          <vuln id="1.3.6.1.4.1.25623.1.0.808160">
            <name>7Zip UDF CInArchive::ReadFileItem Code Execution Vulnerability</name>
            <type>nvt</type>
            <creation_time>2016-06-13T12:57:54+01:00</creation_time>
            <modification_time>2016-12-07T07:43:41+01:00</modification_time>
            <severity>6.8</severity>
            <qod>97</qod>
            <results>
              <count>12</count>
              <oldest>2017-03-21T09:27:44+01:00</oldest>
              <newest>2017-03-21T09:46:42+01:00</newest>
            </results>
            <hosts>
              <count>1</count>
            </hosts>
          </vuln>
          <truncate>...</truncate>
          <filters id="0">
            <term>rows=10 first=1 sort=name</term>
            <keywords>
              <keyword>
                <column>rows</column>
                <relation>=</relation>
                <value>10</value>
              </keyword>
              <keyword>
                <column>first</column>
                <relation>=</relation>
                <value>1</value>
              </keyword>
              <keyword>
                <column>sort</column>
                <relation>=</relation>
                <value>name</value>
              </keyword>
            </keywords>
          </filters>
          <sort>
            <field>
              name

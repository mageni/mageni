            </summary>
            <pattern>
              <attrib>
                <name>id</name>
                <type>uuid</type>
                <summary>UUID of the tag</summary>
                <required>1</required>
              </attrib>
              <e>name</e>
              <e>value</e>
              <e>comment</e>
